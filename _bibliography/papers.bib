@proceedings{jfme2021_tfoldsv,
  
  abbr={EcoSta},
  
  bibtex_show={true},
  
  title={T-fold sequential-validation technique for out-of-distribution generalization with financial time series data},
  
  abstract={The temporal structure in financial time series data demands non-trivial 
    considerations in the use of cross-validation (CV) [1]. Such frequently used technique 
    is based on statistical learning theory, which in turn is founded on the assumption
    that training samples are i.i.d. Although there is progress in the study of fundamental
    phenomenons in certain learning methods such as feature selection imbalance during the
    learning stage [2], currently it is widely accepted that there will be no reason to 
    expect good out of sample results from a learning process without such strong assumption.
    For financial time series data, there are conditions under which sub-sampling data leads 
    to overshadow the effect of non-deterministic relationships between features and the target 
    variable among different samples, such effect remains unnoticed given the use of the additivity 
    property in the decomposition of objective functions for the Learning Process [3], moreover, 
    it reduces to a particular operation the relationship among samples without information attribution,
    in contrast, by using the compositionality principle from a logical and denotational semantics 
    perspective, a broader set of options, by definition, is at hand.
    In this work we present the T-Fold Sequential-Validation technique, a sequential sub- sampling 
    process that controls for information leakage and decomposes the global proba- bility distribution 
    into local probability distributions, it uses a similarity in distribution metric to identify each 
    sample contribution to the learning process, thus maintaining in- formation sparsity and relaxing 
    the effects of the i.i.d. assumption. After sub-sampling, local learning steps are conducted then 
    ssembled to produce a global weighted predic- tion, the voting weights are sequentially updated with a 
    Bayesian criteria for distribution similarity to control for samples coming from previously explored 
    distributions.},

  author={Munoz-Elguezabal J.Francisco , Sanchez-Torres J.Diego},

  year={2021},
  month={Jun},
  publisher=EcoSta,
  doi={10.13140/RG.2.2.30370.24007},
  url={https://hdl.handle.net/11117/7603},
  html={https://www.researchgate.net/publication/353828937_T-Fold_Sequential_Validation_Technique_for_Out-Of-Distribution_Generalization_with_Financial_Time_Series_Data},
  selected={true}

}

@conference{jfme2021_micai,
  abbr={MICAI},
  bibtex_show={true},
  title={Genetic Programming and Neural Networks for Financial Predictive Modeling},
  abstract={Financial time series forecasting is frequently a particularly difficult case for a predictive modeling process, one particular challenge is to capture relationships between features and target variable and to find those whos importance remain invariant given the temporal structure of the data. Genetic programming, can be used as a feature engineering, importance, and selection process that can provide highly interpretable symbolic features, with low colinearity among them and yet high correlation with a target variable. In addition to a Time-Based Sequential Validation Technique for sub-datasets creation to achieve out of distribution generalization for a regression model. In this work is presented the use of such method for generating symbolic features from endogenous linear and autoregressive variables, along with a Multi-Layer Perceptron with a layer of a Gaussian Process Convolution, to construct a regression model to predict daily prices of 3 different cryptocurrencies.},
  author={Munoz-Elguezabal J.Francisco},
  year={2021},
  month={Oct},
  publisher=MICAI,
  selected={true}
}

